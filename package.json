{
  "name": "openapi",
  "version": "0.0.0",
  "private": true,
  "scripts": {    
    "build":"npx yarn install && npx yarn build:server && cd ./vue && npx yarn install && npx yarn build",
    "dev:server": "tsc --watch",
    "build:server": "rm -rf ./routes && tsc",
    "dev:vue":"cd ./vue && npx yarn serve",
    "build:vue":"cd ./vue && npx yarn build",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "axios": "^1.2.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "js-yaml": "^4.1.0",
    "jsdom": "^20.0.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "typescript": "^4.9.3"
  }
}
